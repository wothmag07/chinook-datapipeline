# =============================================================================
# Chinook Data Pipeline - Environment Variables
# Copy this file to .env and fill in the values
# =============================================================================

# --- GCP / BigQuery ---
DBT_BQ_PROJECT=your-gcp-project-id
DBT_BQ_KEYFILE=/path/to/your/gcp-service-account.json

# --- Cube.js (for chinook-dashboard/) ---
CUBEJS_DB_TYPE=bigquery
CUBEJS_DB_BQ_PROJECT_ID=your-gcp-project-id
CUBEJS_DB_BQ_KEY_FILE=/path/to/your/gcp-service-account.json
CUBEJS_DB_BQ_LOCATION=US
CUBEJS_DEV_MODE=true
CUBEJS_API_SECRET=change-me-to-a-random-secret
